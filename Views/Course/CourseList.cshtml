@model List<Course>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>course list</h1>
    @Model[0].Title <br>
    @Model[1].Title <br>
    @Model[2].Title <br>
    <br>
    @foreach(var kurs in Model){
        <div id="kurs_@kurs.Id">
            <h2>@kurs.Title</h2>
            <h2>@kurs.Description</h2>
        </div>
    }
</body>
</html>